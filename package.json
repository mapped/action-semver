{
  "name": "action-vtl",
  "version": "0.2.1",
  "private": true,
  "description": "GitHub Action for establishing a consistent semver",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "lint-fix": "eslint src/**/*.ts --fix",
    "package": "ncc build --source-map --license LICENSE",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mapped/action-semver.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Mapped <info@mapped.com>",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.0.3",
    "@octokit/core": "^4.0.5",
    "@octokit/types": "^8.0.0",
    "@octokit/webhooks": "^10.1.5"
  },
  "devDependencies": {
    "@octokit/openapi-types": "^14.0.0",
    "@octokit/webhooks-examples": "^6.3.6",
    "@octokit/webhooks-methods": "^3.0.0",
    "@octokit/webhooks-types": "^6.3.6",
    "@types/jest": "^29.0.1",
    "@types/node": "^18.0.0",
    "@typescript-eslint/parser": "^5.37.0",
    "@vercel/ncc": "^0.36.0",
    "eslint": "^8.23.1",
    "eslint-plugin-github": "^4.2.0",
    "eslint-plugin-jest": "^27.0.4",
    "jest": "^29.0.3",
    "jest-circus": "^29.0.0",
    "js-yaml": "^4.1.0",
    "prettier": "2.8.1",
    "ts-jest": "^29.0.0",
    "typescript": "^4.4.2"
  }
}
