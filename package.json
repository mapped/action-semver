{
  "name": "mapped-action-semver",
  "version": "0.2.1",
  "private": true,
  "description": "GitHub Action for establishing a consistent semver",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "lint-fix": "eslint src/**/*.ts --fix",
    "package": "ncc build --source-map --license LICENSE",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mapped/action-semver.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Mapped <info@mapped.com>",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.9.1",
    "@actions/github": "^5.0.0",
    "@octokit/core": "^3.5.1",
    "@octokit/types": "^6.27.0",
    "@octokit/webhooks": "^9.14.1"
  },
  "devDependencies": {
    "@octokit/openapi-types": "^10.1.1",
    "@octokit/webhooks-examples": "^4.4.0",
    "@octokit/webhooks-methods": "^2.0.0",
    "@octokit/webhooks-types": "^4.4.0",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.9.0",
    "@typescript-eslint/parser": "^4.31.0",
    "@vercel/ncc": "^0.30.0",
    "eslint": "^7.32.0",
    "eslint-plugin-github": "^4.2.0",
    "eslint-plugin-jest": "^24.4.0",
    "jest": "^27.1.1",
    "jest-circus": "^27.1.1",
    "js-yaml": "^4.1.0",
    "prettier": "2.3.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.2"
  }
}
